/**
    Mux4Way16 gate;
    if sel = 00 then out = a 
    else if sel = 01 then out = b
    else if sel = 10 then out = c
    else if sel = 11 then out = d
*/

CHIP Mux4Way16 {
    IN a[16],b[16],c[16],d[16],sel[2];
    OUT out[16];

    PARTS:
    Mux16(a[0..15]=a[0..15], b[0..15]=b[0..15], sel=sel[0], out[0..15]=temp1);
    Mux16(a[0..15]=c[0..15], b[0..15]=d[0..15], sel=sel[0], out[0..15]=temp2);
    Mux16(a[0..15]=temp1, b[0..15]=temp2, sel=sel[1], out[0..15]=out);
}